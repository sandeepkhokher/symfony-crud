{% extends 'base.html.twig' %}

{% block body %}
    <table class="table table-striped"> 
      <thead> 
        <tr> 
          <th>#</th> 
          <th>First Name</th> 
          <th>Last Name</th> 
          <th>Date</th> 
        </tr> 
      </thead> 
      <tbody> 
               
        {% for user in users %}
        <tr> 
          <th scope="row">{{user.id}}</th> 
            <td>{{user.firstName}}</td> 
            <td>{{user.lastName}}</td> 
            <td>{{user.createdOn|date('Y-m-d')}}</td> 
        </tr> 
        {% endfor %}
        
         
      </tbody> 
    </table>
{% endblock %}
